  },
  "version": "0.1.0",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/pro-light-svg-icons": "^5.1.0",
    "@fortawesome/pro-regular-svg-icons": "^5.1.0",
    "@fortawesome/pro-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "0.0.17",
    "antd": "^3.6.5",
    "deep-equal": "^1.0.1",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "rainbowvis.js": "^1.0.1",
    "react": "^16.4.1",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewired": "^1.5.2",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-lazyload": "^2.3.0",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.2.1",
    "react-scripts": "^1.1.4",
    "react-share": "^2.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "shortid": "^2.2.11",
    "text-encoding": "^0.6.4"
  },
  "scripts": {
    "build-js": "react-app-rewired build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build": "npm-run-all build-css build-js",
    "coverage": "npm run test -- --coverage --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-app-rewired test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "babel-jest": "^21.0.0",
    "babel-plugin-import": "^1.8.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "fake-indexeddb": "^2.0.4",
    "fetch-mock": "^6.5.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "md5": "^2.2.1",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/test/*",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/state/test-files/*",
      "!src/visualization/test-files/*"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./src/test/shim.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
